{
  "metadata":
  [
    {
      "src":
	  [
        {
          "files": [ "ChessSharp/ChessSharp.csproj" ],
		  "src": "../src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build":
  {
    "content":
	[
      {
        "files": [ "api/**.yml", "toc.yml", "index.md" ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "statictoc" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}